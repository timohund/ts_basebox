
; common
expose_php = Off
register_globals = Off
magic_quotes_gpc = Off
sql.safe_mode = Off

; display_errors
display_errors = On

; date time zone
date.timezone = "Europe/Berlin"

; xdebug
xdebug.max_nesting_level = 1000
xdebug.scream = 0

xdebug.profiler_enable = 0
xdebug.profiler_output_dir = "/tmp/"
xdebug.profiler_output_name = "callgrind.%R.%t"